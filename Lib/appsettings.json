{
	"Version": "0.0.1",
	"VisualStudioTemplateVersion": "7.3.3",

	"Logging": {
		"LogLevel": {
			"Default": "Trace",
			"Lib": "Trace",
			"System.Net.Http.HttpClient.Default.ClientHandler": "Warning",
			"Microsoft.Hosting": "Warning",
			"Microsoft.Extensions.Hosting": "Warning"
		},
		"CyberConsole": {
			"DateTimeFormat": "HH:mm:ss",
			"`": "if DateTimeFormat is empty or missing, no date/time information is printed. Full date/time format: yyyy-MM-dd HH:mm:ss.fff",
			"CategoryWidth": 8,
			"``": "No category information is printed if CategoryWidth < 1",

			"Colors": {
				"Enabled": true,

				"None": "Black",
				"Trace": "Gray",
				"Debug": "White",
				"Information": "LimeGreen",
				"Warning": "Orange",
				"Error": "Red",
				"Critical": "Fuchsia"
			}
		}
	}
}